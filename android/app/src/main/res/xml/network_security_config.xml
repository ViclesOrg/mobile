<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <domain-config cleartextTrafficPermitted="true">
        <!-- Allow cleartext traffic only for localhost and 10.0.2.2 -->
        <domain includeSubdomains="true">localhost</domain>
        <domain includeSubdomains="true">10.0.2.2</domain>
        <domain includeSubdomains="true">192.168.1.66</domain>
        <domain includeSubdomains="true">192.168.1.102</domain>
        <domain includeSubdomains="true">192.168.100.215</domain>
        <!-- Add other domains if necessary -->
    </domain-config>
</network-security-config>
